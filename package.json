    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5",
    "vite": "^4.3.0",
    "vite-plugin-dts": "^2.3.0"
  },
  "peerDependencies": {
    "react": "^17.0 || ^18.0",
    "react-dom": "^17.0 || ^18.0"
