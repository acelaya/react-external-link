  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "repository": {
    "url": "https://github.com/acelaya/react-external-link",
    "type": "git"
  },
  "author": {
    "name": "Alejandro Celaya",
